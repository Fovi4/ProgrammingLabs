#include <iostream>
using namespace std;

int main() 
{
    int N, M;
    cin >> N >> M;
    int A[N][M];
    for ( int i = 0; i < N; i++ )
      for ( int j = 0; j < M; j++ )
        cin >> A[i][j];
    for ( int i = 1; i < N; i += 2)
    {
      cout << "Строка " << i+1 << ": ";
      for ( int j = 0; j < M; j++ )
        cout << A[i][j] << " ";
      cout << endl;
    }
}
